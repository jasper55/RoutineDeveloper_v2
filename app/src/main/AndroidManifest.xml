<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app.jasper.routinedeveloper_v2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.app.jasper.routinedeveloper_v2.OverviewActivity"

            android:theme="@style/CustomTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.app.jasper.routinedeveloper_v2.DetailviewActivity"/>
        <activity android:name="com.example.app.jasper.routinedeveloper_v2.model.RecyclerViewAdapter" />

        <receiver android:name="com.example.app.jasper.routinedeveloper_v2.NotificationReceiver"/>


    </application>

    <uses-permission android:name="android.permission.SET_ALARM"></uses-permission>

</manifest>